UND="_"
for PART_SIZE_ENV in 64 128 256 512
do
        for BLOCK_SIZE_ENV in 64 128 256 512
        do

                APP=$PART_SIZE_ENV$UND$BLOCK_SIZE_ENV

#                echo qsub_script_$APP
                /home/carlo/OP2-PaperTests/OP2-Common/apps/c/airfoil/DP/airfoil_cuda_$APP > results_$APP

                sleep 5
                
                mv /home/carlo/OP2-PaperTests/c-tests/times.txt /home/carlo/OP2-PaperTests/c-tests/times_$APP.txt
                mv /home/carlo/OP2-PaperTests/c-tests/results.txt /home/carlo/OP2-PaperTests/c-tests/results_$APP.txt

        done
done
